{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "allowJs": true,
    "baseUrl": "src",
    "paths": {
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}